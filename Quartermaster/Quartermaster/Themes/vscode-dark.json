{
  "manifest_version": "1.0",
  "plugin": {
    "id": "org.quartermaster.theme.vscode-dark",
    "name": "VS Code Dark",
    "version": "1.0.0",
    "author": "Radoub Team",
    "description": "Visual Studio Code inspired dark theme",
    "type": "theme",
    "tags": ["official", "dark", "vscode"]
  },
  "base_theme": "Dark",
  "accessibility": {
    "type": "standard",
    "contrast_level": "AAA"
  },
  "colors": {
    "background": "#1E1E1E",
    "sidebar": "#252526",
    "text": "#CCCCCC",
    "selection": "#264F78",
    "border": "#3C3C3C",
    "accent": "#0078D4",
    "error": "#F48771",
    "warning": "#CCA700",
    "success": "#89D185",
    "info": "#75BEFF",
    "disabled": "#6E6E6E",
    "title_bar": "#323233",
    "title_bar_foreground": "#CCCCCC",
    "button_primary": "#0078D4",
    "button_secondary": "#3C3C3C",
    "button_hover": "#1F9CF0"
  },
  "fonts": {
    "primary": "$Default",
    "monospace": "Consolas",
    "size": 14,
    "weight": "Normal"
  },
  "spacing": {
    "control_padding": 8,
    "control_margin": 4,
    "panel_spacing": 8,
    "min_control_height": 28
  }
}
