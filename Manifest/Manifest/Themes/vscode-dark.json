{
  "manifest_version": "1.0",
  "plugin": {
    "id": "org.manifest.theme.vscode-dark",
    "name": "VS Code Dark",
    "version": "1.0.0",
    "author": "Radoub Team",
    "description": "Visual Studio Code inspired dark theme",
    "type": "theme",
    "tags": ["official", "dark", "vscode"]
  },
  "base_theme": "Dark",
  "accessibility": {
    "type": "standard",
    "contrast_level": "AA"
  },
  "colors": {
    "background": "#1E1E1E",
    "sidebar": "#252526",
    "text": "#CCCCCC",
    "selection": "#264F78",
    "border": "#3C3C3C",
    "accent": "#0078D4",
    "error": "#F14C4C",
    "warning": "#CCA700",
    "success": "#89D185",
    "info": "#3794FF",
    "tree_entry": "#CE9178",
    "tree_reply": "#9CDCFE",
    "tree_link": "#808080",
    "title_bar": "#323233",
    "title_bar_foreground": "#CCCCCC",
    "button_primary": "#0E639C",
    "button_secondary": "#3A3D41",
    "button_hover": "#1177BB"
  },
  "fonts": {
    "primary": "$Default",
    "monospace": "Cascadia Code",
    "size": 14,
    "weight": "Normal"
  },
  "spacing": {
    "control_padding": 8,
    "control_margin": 4,
    "panel_spacing": 8,
    "min_control_height": 28,
    "tree_indent": 16
  }
}
